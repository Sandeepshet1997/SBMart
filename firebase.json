{
    "hosting": {
        "public": "dist/SBMart",
        "ignore": [
            "firebase.json",
            "**/.*",
            "**/node_modules/**"
        ],
        "rewrites": [{
                "source": "/public/**",
                "destination": "/public.html"
            },
            {
                "source": "**",
                "destination": "/index.html"
            }
        ],
        "storage": {
            "rules": "storage.rules"
        }
    }
}